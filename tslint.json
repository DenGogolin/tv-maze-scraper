{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "ordered-imports": false,
    "trailing-comma": false,
    "no-implicit-dependencies": [true, "dev"],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "array-type": false,
    "no-console": false,
    "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"]
  }
}
